{
    "manifest_version": 2,
    "name": "Panelook Select Fix",
    "description": "Restores select and copying functionality on panelook.com",
    "version": "0.0.1",
    "content_scripts": [{
        "js": ["panelook-select-fix.js"],
        "matches": [
            "http://*.panelook.com/*",
            "http://*.panelook.cn/*"
        ]
    }],
    "permissions": [
        "http://*.panelook.com/*",
        "http://*.panelook.cn/*"
    ]
}
